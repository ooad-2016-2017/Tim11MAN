        <RadioButton x:Name="radioButtonDnevni" Content="Dnevni" HorizontalAlignment="Left" Margin="47,335,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="radioButtonMjesecni" Content="Mjesecni" HorizontalAlignment="Left" Margin="213,335,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock1_Copy3" HorizontalAlignment="Left" Margin="48,275,0,0" TextWrapping="Wrap" Text="Registracija vozila" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxRegistracijaVozila" Margin="194,263,27,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top"/>
    </Grid>
</Page>
