        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="48,97,0,0" TextWrapping="Wrap" Text="Ime:" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock1_Copy" HorizontalAlignment="Left" Margin="48,134,0,0" TextWrapping="Wrap" Text="Prezime:" VerticalAlignment="Top"/>
