<Page
    x:Class="ProjekatTaxiAgencijaMAN.RegistracijaMusterije"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ProjekatTaxiAgencijaMAN"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <TextBlock x:ConnectionId='1' x:Name="textBlockImeMusterije" HorizontalAlignment="Left" Margin="26,112,0,0" TextWrapping="Wrap" Text="Ime:" VerticalAlignment="Top"/>
        <TextBlock x:ConnectionId='2' x:Name="textBlockPrezimeMusterije" HorizontalAlignment="Left" Margin="26,138,0,0" TextWrapping="Wrap" Text="Prezime:" VerticalAlignment="Top"/>
        <TextBlock x:ConnectionId='3' x:Name="textBlockDatumRodjenja" HorizontalAlignment="Left" Margin="26,168,0,0" TextWrapping="Wrap" Text="Datum rodjenja:" VerticalAlignment="Top"/>
        <TextBlock x:ConnectionId='4' x:Name="textBlockEmail" HorizontalAlignment="Left" Margin="26,228,0,0" TextWrapping="Wrap" Text="E-mail:" VerticalAlignment="Top"/>
        <TextBlock x:ConnectionId='5' x:Name="textBlockBrojTelefona" HorizontalAlignment="Left" Margin="26,198,0,0" TextWrapping="Wrap" Text="Broj telefona:" VerticalAlignment="Top"/>
        <Button x:ConnectionId='6' x:Name="buttonRegistrujKompaniju" Content="Registruj" HorizontalAlignment="Left" Margin="26,293,0,0" VerticalAlignment="Top" Width="114"/>
        <TextBox x:ConnectionId='7' x:Name="textBoxImeMusterije" HorizontalAlignment="Left" Margin="170,103,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="154" Height="25" MinHeight="10"/>
        <TextBox x:ConnectionId='8' x:Name="textBoxAdresaKompanije" HorizontalAlignment="Left" Margin="170,133,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="154" Height="25" MinHeight="10"/>
        <TextBox x:ConnectionId='9' x:Name="textBoxBrojTelefona" HorizontalAlignment="Left" Margin="170,193,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="154" Height="25" MinHeight="10" RenderTransformOrigin="0.513,1.12"/>
        <TextBox x:ConnectionId='10' x:Name="textBoxEmail" HorizontalAlignment="Left" Margin="170,223,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="154" Height="25" MinHeight="10"/>
        <Button x:ConnectionId='11' x:Name="buttonOdustani" Content="Odustani" HorizontalAlignment="Left" Margin="216,293,0,0" VerticalAlignment="Top" Width="114" Height="32"/>
        <DatePicker x:ConnectionId='12' x:Name="datePickerDatumRodjenja" HorizontalAlignment="Left" Margin="170,163,0,0" VerticalAlignment="Top" Width="154" Height="25" MinWidth="114"/>

    </Grid>
</Page>

